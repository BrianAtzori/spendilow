    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "qrcode": "^1.5.3",
